            <Cursor fx:constant="HAND" />
         </cursor>
      </Text>
      <Text layoutX="203.0" layoutY="382.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Don't have an account?" textAlignment="CENTER" wrappingWidth="191.13671875" />
      <Text layoutX="204.0" layoutY="400.0" onMouseClicked="#onClickSignup" strokeType="OUTSIDE" strokeWidth="0.0" text="Signup" textAlignment="CENTER" underline="true" wrappingWidth="191.13671875">
