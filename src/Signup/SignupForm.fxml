<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane fx:id="signupForm" prefHeight="450.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Signup.SignupFormController">
   <children>
      <TextField fx:id="txtUsername" layoutX="161.0" layoutY="195.0" prefHeight="30.0" prefWidth="276.0" promptText="Enter your username" />
      <PasswordField fx:id="txtReEnterPassword" layoutX="161.0" layoutY="275.0" prefHeight="30.0" prefWidth="276.0" promptText="Re-enter your password" />
      <Button fx:id="loginBtn" layoutX="246.0" layoutY="324.0" mnemonicParsing="false" onAction="#onClickSignup" prefHeight="30.0" prefWidth="106.0" text="Signup">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <PasswordField fx:id="txtPassword" layoutX="161.0" layoutY="235.0" prefHeight="30.0" prefWidth="276.0" promptText="Enter your password" />
      <Text layoutX="204.0" layoutY="376.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Already have an account?" textAlignment="CENTER" wrappingWidth="191.13671875" />
      <Text layoutX="205.0" layoutY="395.0" onMouseClicked="#onClickLogin" strokeType="OUTSIDE" strokeWidth="0.0" text="Login" textAlignment="CENTER" underline="true" wrappingWidth="191.13671875">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Text>
   </children>
</AnchorPane>
